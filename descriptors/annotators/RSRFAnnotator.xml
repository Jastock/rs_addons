    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>

<configurationParameter>
    <name>trained_model_name</name>
    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>

<configurationParameter>
    <name>actual_class_label</name>
